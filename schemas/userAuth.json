{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "examples": [
        {
            "email": "jreidmke@gmail.com",
            "password": "password"
        }
    ],
    "required": [
        "email",
        "password"
    ],
    "properties": {
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "examples": [
                "jreidmke@gmail.com",
                "scifi-movies@scifi.com"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "examples": [
                "password",
                "abc123def456"
            ]
        }
    },
    "additionalProperties": false
}